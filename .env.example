# Configurações do banco de dados (substitua com suas credenciais reais)
DATABASE_URL="postgresql://usuario:senha@host:porta/banco?sslmode=require"

# Configurações do Prisma
PRISMA_GENERATE_DATAPROXY="false"
PRISMA_SCHEMA_DISABLE_ADVISORY_LOCK="true"

# Configurações do Next.js
NEXT_PUBLIC_API_URL="https://seu-site.netlify.app/api"
NEXT_PUBLIC_SITE_URL="https://seu-site.netlify.app"

# Configurações de Autenticação (substitua por valores seguros)
NEXTAUTH_SECRET="seu_segredo_muito_seguro_aqui"
NEXTAUTH_URL="https://seu-site.netlify.app"
JWT_SECRET="seu_super_segredo_jwt"
JWT_EXPIRES_IN="7d"

# Configurações do Netlify
NETLIFY_NEXT_PLUGIN_SKIP="false"

# Configurações do Ambiente
NODE_ENV="production"
NEXT_TELEMETRY_DISABLED="1"

# Configurações do Neon (opcional, se estiver usando)
# NEXT_PUBLIC_STACK_PROJECT_ID=seu_project_id
# NEXT_PUBLIC_STACK_PUBLISHABLE_CLIENT_KEY=sua_chave_publica
# STACK_SECRET_SERVER_KEY=sua_chave_secreta
